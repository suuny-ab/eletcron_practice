{
  "name": "test",
  "version": "1.0.0",
  "description": "eletcron practice",
  "homepage": "https://github.com/suuny-ab/eletcron_practice#readme",
  "bugs": {
    "url": "https://github.com/suuny-ab/eletcron_practice/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/suuny-ab/eletcron_practice.git"
  },
  "license": "ISC",
  "author": "sunny-ab",
  "type": "commonjs",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build:renderer": "npm --prefix frontend run build",
    "build:pydeps": "python -m pip install -r backend/requirements.txt --target backend/.pydeps --upgrade",
    "build:all": "npm run build:pydeps && npm run build:renderer",
    "pack": "npm run build:all && electron-builder --dir",
    "dist": "npm run build:all && electron-builder",
    "dist:win": "npm run build:all && electron-builder --win",
    "clean:pydeps": "rmdir /s /q backend\\.pydeps 2>nul || echo Done",
    "clean:all": "npm run clean:pydeps && rmdir /s /q frontend\\dist 2>nul || echo Done",
    "test": "https://github.com/suuny-ab/eletcron_practice"
  },
  "devDependencies": {
    "electron": "^40.2.1",
    "electron-builder": "^25.1.8"
  },
  "build": {
    "appId": "com.example.ai-knowledge-assistant",
    "productName": "AI 知识库助手",
    "files": [
      "main.js",
      "frontend/dist/**/*",
      "backend/**/*",
      "package.json",
      "diagnose.bat",
      "INSTALL_TROUBLESHOOTING.md"
    ],
    "asarUnpack": [
      "backend/**",
      "diagnose.bat",
      "INSTALL_TROUBLESHOOTING.md"
    ],
    "directories": {
      "buildResources": "build"
    },
    "extraResources": [
      {
        "from": "python",
        "to": "python",
        "filter": ["**/*"]
      }
    ],
    "win": {
      "signAndEditExecutable": false
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    }
  },
  "dependencies": {
    "react-markdown": "^10.1.0",
    "rehype-highlight": "^7.0.2",
    "rehype-raw": "^7.0.0",
    "remark-gfm": "^4.0.1"
  }
}
